		 ef=cd[i];
		 System.out.print("cd[0] ");
		}
		String []mn=ef.split("");
		//System.out.print("cd[0] ");
	
		

for(int j=mn.length-1;j>=0;j--)
{
	System.out.print(mn[j]);
}
	
	}

