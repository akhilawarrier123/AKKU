package p1;

public class String1 {

	public static void main(String[] args) 
	{
		String a = "Kolkata";
		//System.out.println(a.length());		
		String[]vb=a.split("");
		System.out.println(vb.length);
		int i,length=0;
		for(int i=0;i<vb.length);i++);
{
	length++;
}
System.out.println(""+length);
	}

}
