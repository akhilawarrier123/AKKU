package p1;

public class String6 
{

	public static void main(String[] args) 
	{
		String ab= "The Departed";
		String[]cd= ab.split(" ");
		String ef=null;
		String gh=null;
		for(int i=1;i<=cd.length;i++)
		{
		ef=cd[i];
		}
		System.out.println(cd[0]" ");
		
String []mn=ef.split("");
for(int j=mn.length-1;j>=0;j--)
{
	System.out.println(mn[j]);
}
	
	}

}
