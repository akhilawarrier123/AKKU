package p1;

public class String2 
{
	//public class length
	//{
		Static int customLength(String abc)
		{
			String[]vb=abc.split("");
			int count=0;
			for(int i=0;i<vb.length;i++)
			{
				if(i<vb.length)
				{
					count++;
					
				}
			}
		return count
	}
	    		

	public static void main(String[] args) {
	String xc= "Bangalore";
	String[] vb=xc.split("");
	System.out.println(vb.length);
	System.out.println(customLength("Chennai"));

	}

}
